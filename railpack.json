{
    "$schema": "https://schema.railpack.com",
    "steps": {
        "install": {
            "commands": ["pip install -r requirements.txt"]
        }
    },
    "deploy": {
        "inputs": [
            {
                "include": ["."],
                "step": "install"
            }
        ],
        "startCommand": "python app.py"
    },
    "provider": "python",
    "packages": {
        "python": "3.13"
    }
  }